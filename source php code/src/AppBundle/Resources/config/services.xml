<?xml version="1.0" ?>
<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
        <service id="App\AppBundle.form.type.feedback" class="App\AppBundle\Form\FeedbackType">
            <tag name="form.type"/>
        </service>

        <service id="App\AppBundle.form.type.pet_medical_center" class="App\AppBundle\Form\PetMedicalCenterType">
            <tag name="form.type"/>
        </service>

        <service id="App\AppBundle.handler.create_pet_medical_center" class="App\AppBundle\Application\Handler\CreatePetMedicalCenterHandler">
            <argument type="service" id="repository.medical_center"/>
            <argument type="service" id="event_dispatcher"/>
        </service>

        <service id="my_elasticsearch_formatter" class="App\AppBundle\Services\MyFormatter">
            <argument type="string">monolog</argument>
            <argument type="string">logs</argument>
        </service>
    </services>
</container>
